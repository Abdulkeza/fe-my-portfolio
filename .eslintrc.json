{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:react/recommended", "airbnb", "prettier"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"prefer-arrow-callback": 0,
		"no-underscore-dangle": 0,
		"react/jsx-filename-extension": 0,
		"no-undef": 0,
		"import/extensions": [
			"error",
			"ignorePackages",
			{
				"js": "always",
				"jsx": "never"
			}
		]
	}
}
